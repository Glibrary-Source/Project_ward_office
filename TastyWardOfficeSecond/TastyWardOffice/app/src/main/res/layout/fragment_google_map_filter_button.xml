                android:layout_width="70dp"
                android:layout_height="30dp"
                android:layout_marginTop="5dp"
                android:background="@drawable/border_list_imgview"
                android:text="@string/ddm"
                android:textColor="@color/backgroundLightYellowTextColor"
                android:textSize="18dp"
                android:textStyle="bold" />

            <Button
                android:id="@+id/filter_button_dongjak"
                android:layout_width="70dp"
                android:layout_height="30dp"
                android:layout_marginTop="5dp"
                android:background="@drawable/border_list_imgview"
                android:text="@string/dongjak"
                android:textColor="@color/backgroundLightYellowTextColor"
                android:textSize="18dp"
                android:textStyle="bold" />

            <Button
                android:id="@+id/filter_button_mapo"
                android:layout_width="70dp"
                android:layout_height="30dp"
                android:layout_marginTop="5dp"
                android:layout_marginBottom="1dp"
                android:background="@drawable/border_list_imgview"
                android:text="@string/mapo"
                android:textColor="@color/backgroundLightYellowTextColor"
                android:textSize="18dp"
                android:textStyle="bold" />
            <!--15개 구-->
        </LinearLayout>

        <LinearLayout
            android:id="@+id/layout_expand3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@android:color/transparent"
            android:orientation="vertical"
            android:visibility="gone">

            <Button
                android:id="@+id/filter_button_sdm"
                android:layout_width="70dp"
                android:layout_height="30dp"
                android:layout_marginStart="5dp"
                android:layout_marginTop="5dp"
                android:background="@drawable/border_list_imgview"
                android:text="@string/sdm"
                android:textColor="@color/backgroundLightYellowTextColor"
                android:textSize="18dp"
                android:textStyle="bold" />

            <Button
