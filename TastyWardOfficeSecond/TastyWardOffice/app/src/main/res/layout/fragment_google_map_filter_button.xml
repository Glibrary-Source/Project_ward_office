                android:layout_width="70dp"
                android:layout_height="30dp"
                android:layout_marginStart="5dp"
                android:layout_marginTop="5dp"
                android:background="@drawable/border_list_imgview"
                android:text="@string/ydp"
                android:textColor="@color/backgroundLightYellowTextColor"
                android:textSize="18dp"
                android:textStyle="bold" />

            <Button
                android:id="@+id/filter_button_yongsan"
                android:layout_width="70dp"
                android:layout_height="30dp"
                android:layout_marginStart="5dp"
                android:layout_marginTop="5dp"
                android:background="@drawable/border_list_imgview"
                android:text="@string/yongsan"
                android:textColor="@color/backgroundLightYellowTextColor"
                android:textSize="18dp"
                android:textStyle="bold" />

            <Button
                android:id="@+id/filter_button_ep"
                android:layout_width="70dp"
                android:layout_height="30dp"
                android:layout_marginStart="5dp"
                android:layout_marginTop="5dp"
                android:background="@drawable/border_list_imgview"
                android:text="@string/ep"
                android:textColor="@color/backgroundLightYellowTextColor"
                android:textSize="18dp"
                android:textStyle="bold" />

            <Button
                android:id="@+id/filter_button_jongno"
                android:layout_width="70dp"
                android:layout_height="30dp"
                android:layout_marginStart="5dp"
                android:layout_marginTop="5dp"
                android:background="@drawable/border_list_imgview"
                android:text="@string/jongno"
                android:textColor="@color/backgroundLightYellowTextColor"
                android:textSize="18dp"
                android:textStyle="bold" />

            <Button
                android:id="@+id/filter_button_junggu"
                android:layout_width="70dp"
                android:layout_height="30dp"
                android:layout_marginStart="5dp"
                android:layout_marginTop="5dp"
                android:background="@drawable/border_list_imgview"
